    "start-pm2": "pm2 start npm --name 'nuxt-dist' -- run start",
    "generate": "nuxt generate",
    "add-domain": "echo \"eodiro.com\" > dist/CNAME",
    "deploy": "gh-pages -d dist",
    "dist": "npm run build && npm run deploy",
    "test": "mocha --require @babel/register"
