{
  "name": "eodiro",
  "version": "2.1.0-beta.22",
  "description": "CAU Utility Service",
  "scripts": {
    "test": "mocha --require @babel/register",
    "build": "nuxt build",
    "dev": "cross-env HOST=0.0.0.0 PORT=3020 NODE_ENV=development nuxt",
    "start": "cross-env HOST=0.0.0.0 nuxt start",
    "start-dev": "nuxt start --port 3020",
    "pm2-start": "if pm2 show eodiro-client-dist ; then npm run pm2-stop && npm run pm2-delete && npm run pm2-start ; else pm2 start npm --name 'eodiro-client-dist' -- run start ; fi",
    "pm2-restart": "if pm2 show eodiro-client-dist ; then pm2 restart eodiro-client-dist ; else echo 'No pm2 instance for eodiro client' ; fi",
    "pm2-stop": "if pm2 show eodiro-client-dist ; then pm2 stop eodiro-client-dist ; else echo 'No pm2 instance for eodiro client' ; fi",
    "pm2-delete": "if pm2 show eodiro-client-dist ; then pm2 delete eodiro-client-dist ; else echo 'No pm2 instance for eodiro client' ; fi",
    "pm2-reboot": "npm run pm2-stop && npm run pm2-delete && npm run build && npm run pm2-start",
    "checkout-latest": "git checkout $(git describe --tags `git rev-list --tags --max-count=1`)",
    "deploy": "git reset --hard && git fetch --prune && npm run checkout-latest && npm install && npm run pm2-reboot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paywteam/eodiro.git"
  },
  "author": "PAYW",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paywteam/eodiro/issues"
  },
  "homepage": "https://github.com/paywteam/eodiro#readme",
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@vue/composition-api": "^0.3.4",
    "axios": "^0.19.1",
    "body-scroll-lock": "^2.6.4",
    "cookie": "^0.4.0",
    "dayjs": "^1.8.19",
    "disable-scroll": "^0.5.1",
    "intersection-observer": "^0.7.0",
    "js-cookie": "^2.2.1",
    "lorem-ipsum": "^2.0.3",
    "moment": "^2.24.0",
    "mustache": "^3.2.1",
    "nuxt": "^2.11.0",
    "nuxt-i18n": "^6.4.1",
    "platform": "^1.3.5",
    "popmotion": "^8.7.1",
    "simplebar": "^5.1.0"
