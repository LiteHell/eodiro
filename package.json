  "description": "CAU Utility Service",
  "scripts": {
    "dev": "cross-env HOST=0.0.0.0 PORT=3000 NODE_ENV=development nuxt",
    "dev-hcj": "cross-env HOST=0.0.0.0 PORT=3200 nuxt",
    "start-hcj": "HOST=0.0.0.0 PORT=3200 nuxt start",
    "build": "nuxt build",
    "start": "cross-env HOST=0.0.0.0 nuxt start",
    "serve": "serve dist --listen=3000",
