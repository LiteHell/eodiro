{
  "name": "eodiro",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "node_modules/.bin/webpack --config webpack.config.js",
    "build:watch": "node_modules/.bin/webpack -w --config webpack.config.js",
    "build:prod": "node_modules/.bin/webpack --config webpack.prod.js",
    "build:prod:watch": "node_modules/.bin/webpack -w --config webpack.prod.js",
    "build:fast:watch": "node_modules/.bin/webpack -w --config webpack.fast.js",
    "build-s": "node_modules/.bin/webpack --config webpack.server.js",
    "build-s:watch": "node_modules/.bin/webpack -w --config webpack.server.js",
    "build-all": "npm run build && npm run build-s",
    "build-all:watch": "concurrently --kill-others \"npm run build:watch\" \"npm run build-s:watch\"",
    "start": "forever start ./forever.json",
    "stop-web": "forever stop eodiro_web",
    "stop-dev-jhm": "forever stop eodiro_dev_jhm",
    "stop-dev-hch": "forever stop eodiro_dev_hch",
    "stop-dev-hcj": "forever stop eodiro_dev_hcj",
    "stop-beta": "forever stop eodiro_beta",
    "stop-demo": "forever stop eodiro_demo"
  },
